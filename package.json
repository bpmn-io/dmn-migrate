{
  "name": "@bpmn-io/dmn-migrate",
  "version": "0.5.0",
  "description": "Migrate DMN diagrams to the latest DMN standard.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "source": "src/migrateDiagram.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "distro": "run-s build test:build",
    "lint": "eslint .",
    "prepublishOnly": "run-s distro",
    "test": "mocha -r esm --reporter=spec --recursive test/spec",
    "test:build": "mocha --reporter=spec --recursive test/distro",
    "all": "run-s lint test distro"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bpmn-io/dmn-migrate.git"
  },
  "keywords": [
    "dmn",
    "migration"
  ],
  "author": "Philipp Fromme",
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "dmn-moddle": "^10.0.0",
    "ids": "^1.0.5",
    "min-dash": "^4.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.56.0",
    "eslint-plugin-bpmn-io": "^1.0.0",
    "esm": "^3.2.25",
    "execa": "^4.0.0",
    "mocha": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^4.9.4",
    "xsd-schema-validator": "^0.7.0"
  },
  "files": [
    "dist"
  ]
}
